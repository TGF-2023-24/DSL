  </pn_150>
  <featuremodel>
    <nodes name="Wash"
        root="true"/>
    <nodes name="Heat"
        requires="Wash"/>
    <nodes name="Delay"
        requires="Wash"/>
    <nodes name="Dry"
        requires="Wash"/>
    <relations
        parent="Wash"
        childs="Heat Delay Dry"/>
  </featuremodel>
  <presenceconditions
      id="PC_Heat">
    <expression
        xsi:type="pnpl:Feature"
        feature="Heat"/>
  </presenceconditions>
  <presenceconditions
      id="PC_Delay">
    <expression
        xsi:type="pnpl:Feature"
        feature="Delay"/>
  </presenceconditions>
  <presenceconditions
      id="PC_Dry">
    <expression
        xsi:type="pnpl:Feature"
        feature="Dry"/>
  </presenceconditions>
  <presenceconditions
      id="PC_NotDry">
    <expression
        xsi:type="pnpl:UnaryExpression">
      <right
          xsi:type="pnpl:Feature"
          feature="Dry"/>
    </expression>
  </presenceconditions>
  <presenceconditions
      id="PC_HeatOrDelay">
    <expression
        xsi:type="pnpl:BinaryExpression"
        operator="OR">
      <left
          xsi:type="pnpl:Feature"
          feature="Heat"/>
      <right
          xsi:type="pnpl:Feature"
          feature="Delay"/>
    </expression>
  </presenceconditions>
</pnpl:PNPL>
