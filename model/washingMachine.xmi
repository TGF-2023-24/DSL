    <elements
        xsi:type="pnpl:ArcPT"
        presenceCondition="PC_HeatOrDelay"
        name="a3"
        source="p_locking"
        target="t_cycle"/>
    <elements
        xsi:type="pnpl:ArcTP"
        presenceCondition="PC_Heat"
        name="a4"
        source="t_cycle"
        target="p_heaterOn"/>
    <elements
        xsi:type="pnpl:ArcTP"
        presenceCondition="PC_HeatOrDelay"
        name="a5"
        source="t_cycle"
        target="p_waiting"/>
    <elements
        xsi:type="pnpl:ArcPT"
        presenceCondition="PC_HeatOrDelay"
        name="a6"
        source="p_waiting"
        target="t_waiting"/>
    <elements
        xsi:type="pnpl:ArcTP"
        presenceCondition="PC_HeatOrDelay"
