<?xml version="1.0" encoding="UTF-8"?>
<RedDePetri:FeatureModel
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:RedDePetri="http://RedDePetri">
  <formula xsi:type="RedDePetri:Binary">
    <left xsi:type="RedDePetri:Base"
        feature="VendingMachine"/>
    <right xsi:type="RedDePetri:Binary">
      <left
          xsi:type="RedDePetri:Base"
          feature="Container"/>
      <right
          xsi:type="RedDePetri:Binary">
        <left
            xsi:type="RedDePetri:Binary"
            op="or">
          <left
              xsi:type="RedDePetri:Base"
              feature="Beverage"/>
          <right
              xsi:type="RedDePetri:Base"
              feature="Solid"/>
        </left>
        <right
            xsi:type="RedDePetri:Binary">
          <left
              xsi:type="RedDePetri:Binary"
              op="double_implication">
            <left
                xsi:type="RedDePetri:Base"
                feature="Beverage"/>
            <right
                xsi:type="RedDePetri:Binary"
                op="or">
              <left
                  xsi:type="RedDePetri:Base"
                  feature="Tea"/>
              <right
                  xsi:type="RedDePetri:Base"
                  feature="Coffee"/>
            </right>
          </left>
          <right
              xsi:type="RedDePetri:Binary">
            <left
                xsi:type="RedDePetri:Binary"
                op="double_implication">
              <left
                  xsi:type="RedDePetri:Base"
                  feature="Supplement"/>
              <right
                  xsi:type="RedDePetri:Binary"
                  op="or">
                <left
                    xsi:type="RedDePetri:Base"
                    feature="Milk"/>
                <right
                    xsi:type="RedDePetri:Base"
                    feature="Sugar"/>
              </right>
            </left>
            <right
                xsi:type="RedDePetri:Binary">
              <left
                  xsi:type="RedDePetri:Binary"
                  op="implies">
                <left
                    xsi:type="RedDePetri:Base"
                    feature="Sugar"/>
                <right
                    xsi:type="RedDePetri:Base"
                    feature="Beverage"/>
              </left>
              <right
                  xsi:type="RedDePetri:Binary"
                  op="implies">
                <left
                    xsi:type="RedDePetri:Base"
                    feature="Milk"/>
                <right
                    xsi:type="RedDePetri:Base"
                    feature="Coffee"/>
              </right>
            </right>
          </right>
        </right>
      </right>
    </right>
  </formula>
  <features
      name="VendingMachine"/>
  <features
      name="Container"/>
  <features
      name="Supplement"/>
  <features
      name="Beverage"/>
  <features
      name="Solid"/>
  <features
      name="Tea"/>
  <features
      name="Coffee"/>
  <features
      name="Milk"/>
  <features
      name="Sugar"/>
</RedDePetri:FeatureModel>
