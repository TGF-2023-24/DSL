<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="RedDePetri" nsURI="http://RedDePetri" nsPrefix="RedDePetri">
  <eClassifiers xsi:type="ecore:EClass" name="RdP">
    <eStructuralFeatures xsi:type="ecore:EReference" name="transiciones" lowerBound="1"
        upperBound="-1" eType="#//Transicion" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="lugares" lowerBound="1"
        upperBound="-1" eType="#//Lugar" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="arcos" lowerBound="1" upperBound="-1"
        eType="#//Arco" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Lugar">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
        iD="true"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="marca" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Transicion">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="name" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EString"
        iD="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Arco">
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="peso" lowerBound="1" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ArcoTL" eSuperTypes="#//Arco">
    <eStructuralFeatures xsi:type="ecore:EReference" name="origen" lowerBound="1"
        eType="#//Transicion" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="destino" lowerBound="1"
        eType="#//Lugar" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="ArcoLT" eSuperTypes="#//Arco">
    <eStructuralFeatures xsi:type="ecore:EReference" name="origen" lowerBound="1"
        eType="#//Lugar" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="destino" lowerBound="1"
        eType="#//Transicion" containment="true"/>
  </eClassifiers>
</ecore:EPackage>
